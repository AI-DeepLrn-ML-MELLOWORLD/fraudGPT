
{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "app/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/v1/(.*)",
        "dest": "app/index.js"
      },
      {
        "src": "/",
        "dest": "public/index.html"
      }
    ]
  }
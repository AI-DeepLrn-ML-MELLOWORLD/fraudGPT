
{
    "version": 2,

    "env": {
    },
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    "builds": [
      {
        "src": "app/index.js",
        "use": "@vercel/node"
      },{
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/v1/(.*)",
        "dest": "app/index.js"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      },
      {
        "src": "/",
        "dest": "public/index.html"
      }
    ]
  }